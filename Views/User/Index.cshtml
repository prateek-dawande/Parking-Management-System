@model List<ParkingManagementSystem.Models.Domain.User>
@{
    var i = 1;
}
<h1>All Users</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">S.No</th>
            <th scope="col">User Name</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
            i++;
        }


    </tbody>
</table>